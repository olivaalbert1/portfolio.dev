---
import JavaScriptIcon from '../assets/icons/JavaScriptIcon.astro'
import PythonIcon from '../assets/icons/PythonIcon.astro'

import 'aos/dist/aos.css'




const TAGS = {
    NEXT: {
        name: 'Next.js',
        class: 'bg-black text-white',
        icon: ''
    },
    TAILWIND: {
        name: 'Tailwind CSS',
        class: 'bg-white text-[#73bcf6]',
        icon: ''
    },
    JAVASCRIPT: {
        name: 'JavaScript',
        class: 'bg-black text-white',
        icon: JavaScriptIcon
    },
    TYPESCRIPT: {
        name: 'TypeScript',
        class: 'bg-black text-white',
        icon: ''
    },
    REACT: {
        name: 'React',
        class: 'bg-black text-white',
        icon: ''
    },
    PYTHON: {
        name: 'Python',
        class: 'bg-black text-white',
        icon: PythonIcon
